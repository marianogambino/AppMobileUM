<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.edu.unimoron.appmobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="14" />

    
    <!--  @style/AppTheme 
    	  android:style/Theme.Holo.Ligh
    	  
    	  @android:style/Theme.Holo.Light
    	  @style/CustomActionBarTheme    
   -->
   
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name="ar.edu.unimoron.appmobile.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="ar.edu.unimoron.appmobile.ShowMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="ar.edu.unimoron.appmobile.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ar.edu.unimoron.appmobile.MainActivity" />
        </activity>
    </application>

</manifest>
